<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE configuration PUBLIC "-//mybatis.org//DTD Config 3.0//EN" "http://mybatis.org/dtd/mybatis-3-config.dtd">

<configuration>
    <typeAliases>
        <typeAlias alias="InquiryVo" type="com.lunchwb.vo.InquiryVo"/>
   		<typeAlias alias="FaqVo" type="com.lunchwb.vo.FaqVo"/>
   		<typeAlias alias="UserVo" type="com.lunchwb.vo.UserVo"/>
